{
  "name": "webmap-sdk",
  "version": "2.8.1",
  "description": "Webmap SDK",
  "files": [
    "/dist/*"
  ],
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js --open --open-page 'build/examples'",
    "serve": "webpack-dev-server --config webpack-dev-server.config.js",
    "pretest": "npm run check-docs ; npm run lint",
    "test": "jest --maxWorkers=2",
    "test:watch": "npm test -- --watch",
    "lint": "eslint __tests__ src examples tasks",
    "lint-fix": "eslint __tests__ src examples tasks --fix",
    "cover": "node_modules/.bin/jest --coverage",
    "sass": "node-sass --include-path ./node_modules ./src/stylesheet/sdk.scss",
    "build:examples": "node tasks/build-examples.js",
    "build:hosted:examples": "node tasks/build-examples.js build/hosted/examples",
    "build:examples-js": "webpack --define process.env.NODE_ENV='\"production\"' --config ./webpack-example.config.js",
    "build:css": "npm run sass -- -o ./build/hosted/examples",
    "bundle-examples": "npm run build:hosted:examples && npm run build:examples-js && npm run build:css",
    "build:js": "babel ./src --out-dir ./dist",
    "watch:js": "babel --watch ./src --out-dir ./dist",
    "dist:copy": "node tasks/dist-copy.js && node tasks/create-package-json.js",
    "dist": "npm run build:js && npm run dist:copy && npm run sass -- -o ./dist",
    "build:doc-css": "npm run sass -- -o ./docs",
    "build:jsdoc": "jsdoc -r ./src -R docs/getting-started.md -d docs/out -c docs/conf.json",
    "jsdoc": "npm run build:doc-css && npm run build:jsdoc",
    "check-docs": "cd docs && ./doc_checker.sh"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@mapbox/mapbox-gl-draw": "^1.1.1",
    "@mapbox/mapbox-gl-draw-static-mode": "^1.0.1",
    "@turf/area": "^5.0.4",
    "@turf/distance": "^5.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-react-transform": "^3.0.0",
    "canvas": "^1.6.8",
    "coveralls": "^2.13.1",
    "css-loader": "0.28.7",
    "deep-freeze": "0.0.1",
    "dotenv": "^4.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.10.0",
    "eslint-config-openlayers": "^7.0.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs-extra": "^4.0.2",
    "handlebars": "^4.0.10",
    "jest": "^20.0.4",
    "jest-cli": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "jsdoc": "^3.5.4",
    "mapbox-gl": "^0.44.2",
    "marked": "^0.3.6",
    "metalsmith": "^2.3.0",
    "metalsmith-layouts": "^1.8.1",
    "metalsmith-rootpath": "^1.0.4",
    "mixin": "^0.2.0",
    "nock": "^9.0.13",
    "node-sass": "^4.12",
    "ol": "^5.0.0",
    "ol-mapbox-style": "^3.0.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.0.0",
    "react-redux": "^6.0.0",
    "react-test-renderer": "^16.0.0",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "xmldom": "^0.1.27"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-style-spec": "^12.0.0",
    "fetch-jsonp": "^1.1.3",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.7.2",
    "react-docgen": "^2.19.0",
    "redux-saga": "^0.16.0"
  },
  "peerDependencies": {
    "@mapbox/mapbox-gl-draw": "^1.0.4",
    "@mapbox/mapbox-gl-draw-static-mode": "^1.0.1",
    "@turf/area": "^5.0.4",
    "@turf/distance": "^5.0.4",
    "ol-mapbox-style": "^3.0.1",
    "react": "^16.0.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planetfederal/sdk.git"
  },
  "author": "Planet Federal",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/planetfederal/sdk/issues"
  },
  "homepage": "https://github.com/planetfederal/sdk#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testURL": "http://localhost",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(ol|ol-mapbox-style|@mapbox/mapbox-gl-style-spec)/).*/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "env-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
